
.profile-user-image-login{
  width: 40px;
  height: 40px;
  margin-top: 20px;
  margin-left: 20px;
  margin-right: 5px;
  align-items: center;
  border-radius: 50%;
  background-color: rgb(114, 114, 114);
  border-style: solid;
  border-width: 1px;
  border-color: rgb(255, 255, 255);
  overflow: hidden;
}
.first-info-panel{
  width: 95%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  background-color: white;
  margin-top: 10px;
  margin-bottom: 5px;
  margin-left: 20px;
  margin-right: 20px;
  border-radius: 5px;
  padding-right: 10px;
  padding-left: 10px;
  
}
.profile_item_icon {
  width: 15px;
  height: 15px;
}
.profile-item-name{
  font-size: 13px;
  font-weight: 550;
  font-style: normal;
  text-transform: uppercase;
  color: #0D0D0D;
  display: flex;
  margin-left: 10px;
  margin-right: auto;
}
.profile_item_icon{
  margin-left: auto;
  margin-right: 10px;
  display: flex;
  align-self: center;
  left: 0px;
}
.item-list-view{
  margin-top: 20px;
  margin-bottom: 30px;
  display: flex;
  flex-direction: raw;
  width: 100%;
  text-align-last: center;
}
.item-view{
  width: 50px;
  height: 70px;
  margin-left: auto;
  margin-right: auto;
}
.item-subview{
  font-size: 12px;
  text-align: center;
  color: #A3A3A3;
  font-weight: 550;
  text-transform: uppercase;
}
.item-parent-view{
  background-color: rgb(196, 255, 58);
  border-radius: 50%;
  width: 50px;
  height: 50px;
  align-items: center;
  display: flex;
}
.item-image{
  margin: auto;
  margin-bottom: auto;
  width: 19px;
  height: 22px;
}
.profile-cell-two{
  display: flex;
  justify-content: space-between;
  margin-top:15px;
  margin-left: 0px;
  margin-right: auto;
  width: 100%;
  align-items: center;
  margin-bottom: 15px;
}
.slide-image{
  border-radius: 10px;
  width: 95%;
  height: 140px;
  align-items: center;
  justify-content: space-between;
  margin-left: auto;
  margin-right: auto;
  /* flex-direction: column;
  display: flex; */
}
.page-body{
  margin-top: 10px;
  height: auto;
}

.level-item-data{
  font-size: 13px;
  font-style: normal;
  color: #707070;
  display: flex;
  margin-left: 10px;
  margin-right: auto;
}