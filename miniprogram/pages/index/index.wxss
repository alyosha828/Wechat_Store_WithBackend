
/**index.wxss**/
.down-icon{
  width: 15px;
  height: 10px;
  margin-left: 5px;
}
.dropdown-layout {
  width: 750rpx;
  overflow: hidden;
  position: fixed;
  top: 30px;
  z-index: 1;
  border-bottom: solid 1px #eee;
  height: 100%;
  background-color: rgba(000,000,000,.5);
  
}
.dropdown-cell{
  width: 750rpx;
  text-indent: 40rpx;
  height: 80rpx;
  line-height: 80rpx;
  border-top: 1rpx solid #e7e7e7;
  background-color:#ffffff;
}
page {
  background-color: #f6f6f6;
  padding-bottom: 60rpx;
}

.title {
  font-family: PingFang SC;
  font-weight: 500;
  color: #000000;
  font-size: 44rpx;
  margin-bottom: 40rpx;
}

.top_tip {
  font-size: 28rpx;
  font-family: PingFang SC;
  font-weight: 400;
  color: #888888;
  margin-bottom: 28rpx;
}
.tabs-container {
  position: relative;
  left: 0;
  top: 0;
  height: 80rpx;
  width: 100%;
  background-color: white;
}
.power {
  margin-top: 5rpx;
  border-radius: 5px;
  background-color: white;
  width: 93%;
  padding-bottom: 1rpx;
}

.power_info {
  display: flex;
  /* padding: 5rpx 25rpx; */
  align-items: center;
  /* justify-content: space-between; */
}

.power_info_more {
  width: 50rpx;
  height: 50rpx;
  margin-left: auto;
  margin-right: 0px;
}

.power_info_less {
  width: 30rpx;
  height: 30rpx;
  transform: rotate(270deg);
}

.power_info_text {
  /* display: flex;
  flex-direction: column; */
  width: 100%;
}

.power_info_text_title {
  font-weight: 400;
  font-size: 30rpx;
}

.power_info_text_tip {
  color: rgba(0, 0, 0, 0.4);
  font-size: 25rpx;
  margin-left: 5px;
  margin-right: 5px;
}

.power_item {
  padding: 30rpx 25rpx;
  display: flex;
  justify-content: space-between;
}

.power_item_title {
  font-size: 30rpx;
}

.power_item_icon {
  width: 30px;
  height: 30px;
}
.room-info-panel{
  width: 95%;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  background-color: white;
  margin-top: 5px;
  margin-left: 20px;
  margin-right: 20px;
  border-radius: 2px;
}
.roomList {
  margin-top: 40rpx;
  border-radius: 5px;
  background-color: white;
  width: 93%;
  padding-bottom: 1rpx;
}

.room_info {
  display: flex;
  align-items: center;
  height: 180px;
  /* justify-content: space-between; */
}

.room_info_image {
  height: 180px;
  width: 140px;
  margin-top: 0px;
  transform: rotate(0deg);
  border-radius: 7px;
}

.room_info_less {
  width: 30rpx;
  height: 30rpx;
  transform: rotate(270deg);
}

.room_info_main {
  display: flex;
  flex-direction: column;
  width: 100%;
}

.room_info_text_title {
  font-weight: 400;
  font-size: 30rpx;
  margin-left: 15rpx;
}

.room_info_text_detail {
  color: rgba(0, 0, 0, 0.4);
  font-size: 25rpx;
  margin-left: 15rpx;
}

.room_info_price {
  margin-top: 10px;
  flex-direction:row;
  display: flex;
}
.flex-item.price-text-1 {
  font-size: 25rpx;
  margin-left: 15rpx;
  width: 50%;
  color: white;
  text-align: center;
  margin-right: 10px;
  background-color: #2C2C2C;
  border-radius: 5px;
}
.flex-item.price-text-2 {
  font-size: 25rpx;
  width: 50%;
  text-align: center;
  color: #A3A3A3;
  border-style: solid;
  border-color: #A3A3A3;
  border-width: 1px;
  border-radius: 5px;
}
.yeding-btn{
  font-size: 14px;
  margin-top: 5px;
  margin-right: 5px;
  background-color: #05C160;
  color: rgb(255, 255, 255);
  text-align: center;
  width: 60px;
  margin-left: auto;
  margin-bottom: 5px;
  padding-left: 10px;
  padding-right: 10px;
  padding-top: 3px;
  padding-bottom: 3px;
  border-radius: 8px;
}

.room_item {
  padding: 30rpx 25rpx;
  display: flex;
  justify-content: space-between;
}

.room_item_title {
  font-size: 30rpx;
}

.room_item_icon {
  width: 30rpx;
  height: 30rpx;
}

.line {
  width: 95%;
  margin: 0 auto;
  height: 2rpx;
  background-color: rgba(0, 0, 0, 0.1);
}

.environment {
  color: rgba(0, 0, 0, 0.4);
  font-size: 24rpx;
  margin-top: 25%;
}

.date-string{
  margin-top: 20rpx;
  background-color: #fff;
  font-size: 28rpx;
  padding: 0 30rpx;
  line-height: 60rpx;
}


